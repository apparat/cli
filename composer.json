{
  "name": "apparat/cli",
  "description": "Apparat command line interface",
  "license": "MIT",
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@kuphal.net"
    }
  ],
  "minimum-stability": "RC",
  "bin": ["src/Cli/Ports/Apparat.php"],
  "bin-dir": "bin",
  "require": {
    "php": ">=5.6.0",
    "apparat/object": "@dev",
    "apparat/resource": "@dev",
    "apparat/kernel": "@dev",
    "dominicsayers/isemail": "@dev",
    "symfony/console": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "satooshi/php-coveralls": "^1.0",
    "codeclimate/php-test-reporter": "dev-master",
    "apparat/dev": "@dev"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:apparat/server.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/object.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/resource.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/kernel.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apparat/dev.git",
      "reference": "master"
    },
    {
      "type": "vcs",
      "url": "git@github.com:dominicsayers/isemail.git",
      "reference": "master"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apparat\\": "src/"
    }
  }
}
